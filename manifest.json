{
  "name": "Arreglando la nacion",
  "version": "1.2.2",
  "description": "Arregla el diseño de los sitios del grupo nacion, además remueve las restricciones de lectura de contenido.",
  "content_scripts": [ {
    "css": [
      "css/escondiendo-elementos.css",
      "css/otros.css"
    ],
    "js": [
      "js/arreglos.js"
    ],
    "matches": [
      "http://www.nacion.com/*",
      "https://www.nacion.com/*"
    ],
    "runat": "document_start"
  } ],
  "permissions": [
    "tabs", "http://www.nacion.com/*"
  ],
  "icons": {
    "16": "img/icon-19.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "manifest_version": 2
}
